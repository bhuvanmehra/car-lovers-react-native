{
  "expo": {
    "name": "Car Lovers",
    "slug": "car-lovers-react-native",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#DADADA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.mehrasoft.carLovers",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mehrasoft.carLovers"
    }
  }
}